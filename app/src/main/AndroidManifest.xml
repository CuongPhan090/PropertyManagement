<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.propertymanagement">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PropertyManagement">
        <activity
            android:name=".view.RegisterActivity"
            android:label="Create an account" />
        <activity
            android:name=".view.ResetPasswordActivity"
            android:label="@string/reset_password" />
        <activity
            android:name=".view.LoginActivity"
            android:exported="true"
            android:label="@string/login_activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".view.DashboardActivity"
            android:label="@string/dashboard" />

        <activity
            android:name=".view.MainActivity"
            android:exported="true">

        </activity>
    </application>

</manifest>